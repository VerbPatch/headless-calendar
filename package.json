{
  "name": "headless-calendar",
  "namesapce": "@verbpatch",
  "type": "module",
  "version": "0.0.1-beta.10",
  "description": "",
  "author": "Rameez Raja",
  "license": "LGPL-3.0-or-later",
  "packageManager": "pnpm@10.18.3",
  "keywords": [],
  "scripts": {
    "clean": "pnpm --filter \"./packages/**\" run clean",
    "build": "pnpm --filter \"./packages/**\" -r build && size-limit",
    "build:examples": "pnpm --filter \"./examples/**\" -r build",
    "build:all": "pnpm -r build && size-limit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "publish": "pnpm --filter \"./packages/**\" -r publish  --no-git-checks --access public --publish-branch main",
    "docs": "typedoc",
    "changeset": "changeset",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "size-limit": [
    {
      "limit": "11 kB",
      "path": "packages/headless-calendar/dist/index.js"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@size-limit/file": "^11.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "rimraf": "^6.1.0",
    "rollup": "^4.53.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-size": "^0.3.1",
    "rollup-plugin-svelte": "^7.2.3",
    "size-limit": "^11.2.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typedoc-plugin-frontmatter": "^1.3.1",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
