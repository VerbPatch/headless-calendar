{
  "name": "qwik-calendar-minimal-example",
  "description": "Qwik App with Routing built with Vite",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.ssr.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview",
    "start": "vite --open",
    "qwik": "qwik"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.17.2",
    "@builder.io/qwik-city": "^1.17.2",
    "@verbpatch/qwik-calendar": "workspace:^"
  },
  "devDependencies": {
    "@builder.io/qwik-react": "^0.5.7",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-qwik": "^1.17.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "undici": "^7.16.0",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
