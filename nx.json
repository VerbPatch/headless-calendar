{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "inputs": [
        "{workspaceRoot}/package.json",
        "{workspaceRoot}/tsconfig.json",
        "{projectRoot}/**/*",
        "!{projectRoot}/**/*.md",
        "!{projectRoot}/dist"
      ],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    }
  }
}
